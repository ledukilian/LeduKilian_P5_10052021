<footer id="footer">
   <div class="footer1">
      <div class="container">
         <div class="row">
            <div class="col-md-12 text-center widget">
               <div class="widget-body">
                  {% for social in socials %}
                     <a class="h1 m-2" href="{{social.link}}" target="_blank">
                        {{social.icon | raw }}
                     </a>
                  {% endfor %}
               </div>
               {% if session.logged == true %}
                     {% if session.user.role == "ADMIN" %}
                        <a class="nav-link mt-4 text-secondary" href="/admin/">Accéder à l'administration</a>
                     {% endif %}
               {% endif %}
            </div>
         </div>
      </div>
   </div>
   <div class="footer2">
      <div class="container">
         <div class="row">
            <div class="col-md-12 widget">
               <div class="widget-body">
                  <p class="text-center">
                     Copyright &copy; 2021 - <b>LE DU Kilian</b>
                  </p>
               </div>
            </div>
         </div>
      </div>
   </div>
</footer>
